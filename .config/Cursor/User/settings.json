{
  "crashReporting.enabled": "off",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "update.mode": "none",
  "window.autoDetectHighContrast": false,
  "window.customTitleBarVisibility": "auto",
  "window.titleBarStyle": "custom",
  "window.restoreWindows": "preserve",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "editorInlayHint.background": "#00000000",
    "editorInlayHint.foreground": "#40434C",
    "editorInlayHint.parameterBackground": "#00000000",
    "editorInlayHint.parameterForeground": "#40434C",
    "editorInlayHint.typeBackground": "#00000000",
    "editorInlayHint.typeForeground": "#40434C",
    "editorRuler.foreground": "#333333",
  },
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.historyBasedLanguageDetection": true,
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.list.defaultFindMode": "highlight",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "breadcrumbs.enabled": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.experimental.treeSitterTelemetry": false,
  "editor.folding": false,
  "editor.fontFamily": "'Monaspace Neon Var', monospace, 'Noto Emoji', 'Noto Color Emoji'",
  "editor.fontLigatures": "'calt', 'liga'",
  "editor.fontSize": 15,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "onUnlessPressed",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 100],
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.deprecated": {
        "strikethrough": true,
      },
    },
  },
  "editor.snippetSuggestions": "top",
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": "allDocuments",
  "editor.wordWrap": "on",
  "debug.confirmOnExit": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".env.*": "properties",
  },
  "files.exclude": {
    "**/.*cache": true,
    "**/.git": true,
    "**/.jj": true,
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.*cache": true,
    "**/.*cache/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.jj/**": true,
    "**/.turbo/**": true,
    "**/.wrangler/**": true,
    "**/*-lock.*": true,
    "**/*.bak": true,
    "**/*.bak/**": true,
    "**/*.lock": true,
    "**/build/**": true,
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/node_modules/**": true,
    "**/playwright-report/**": true,
    "**/public/**": true,
    "**/test-results/**": true,
    "**/tmp/**": true,
    "**/var/**": true,
    "**/zig-out/**": true,
  },
  "search.seedWithNearestWord": true,
  "search.smartCase": true,
  "search.quickOpen.includeSymbols": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.detectSubmodules": false,
  "git.enableCommitSigning": true,
  "git.mergeEditor": false,
  "git.suggestSmartCommit": false,
  "scm.alwaysShowRepositories": true,
  "debug.allowBreakpointsEverywhere": false,
  "terminal.integrated.defaultProfile.linux": "bash",
  // "terminal.integrated.defaultProfile.linux": "sandbox",
  // "terminal.integrated.profiles.linux": {
  //   "sandbox": {
  //     "path": "/home/max/.local/bin/sandbox.sh",
  //     "args": [],
  //   },
  // },
  // "terminal.integrated.shellIntegration.enabled": false,
  "task.allowAutomaticTasks": "off",
  "task.autoDetect": "off",
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "jake.autoDetect": "off",
  "npm.autoDetect": "off",
  "npm.packageManager": "bun",
  "npm.scriptRunner": "bun",
  "html.completion.attributeDefaultValue": "empty",
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.npm": "bun",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.surveys.enabled": false,
  "typescript.tsc.autoDetect": "off",

  "biome.enabled": false,
  "biome.requireConfiguration": true,
  "biome.suggestInstallingGlobally": false,
  "cursor.composer.shouldChimeAfterChatFinishes": true,
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "cursor.cpp.enablePartialAccepts": true,
  "harper.codeActions.ForceStable": true,
  "harper.dialect": "British",
  "harper.isolateEnglish": true,
  "redhat.telemetry.enabled": false,
  "zig.path": "zig",
  "zig.zls.enabled": "on",
  "zig.zls.path": "zls",
  "zig.zls.warnStyle": true,

  "[markdown]": {
    "editor.lineNumbers": "interval",
    "editor.rulers": [80],
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "indent",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off",
    },
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": false,
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.autoIndent": "advanced"
  },
  "[zig]": {
    "editor.defaultFormatter": "ziglang.vscode-zig",
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "editor.suggest.insertMode": "replace",
    "editor.stickyScroll.defaultModel": "foldingProviderModel",
  },
}
